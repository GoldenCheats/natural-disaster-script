    CreateESP(p) 
    p.Chatted:Connect(function(msg) HandleCommand(msg, p) end) 
end

Players.PlayerAdded:Connect(function(p) 
    CreateESP(p) 
    p.Chatted:Connect(function(msg) HandleCommand(msg, p) end) 
    RefreshList() 
end)

